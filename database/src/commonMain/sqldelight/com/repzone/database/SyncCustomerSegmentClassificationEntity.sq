    ClassificationId INTEGER,
    ClassificationName TEXT,
    CustomerId INTEGER,
    ModificationDateUtc INTEGER,
    OrganizationId INTEGER,
    RecordDateUtc INTEGER,
    ShareStatus INTEGER,
    State INTEGER
);

selectAllSyncCustomerSegmentClassificationEntity:
SELECT * FROM SyncCustomerSegmentClassificationEntity;

selectBySyncCustomerSegmentClassificationEntityId:
SELECT * FROM SyncCustomerSegmentClassificationEntity WHERE ClassificationId = :ClassificationId;

insertSyncCustomerSegmentClassificationEntity:
INSERT INTO SyncCustomerSegmentClassificationEntity (ClassificationId, ClassificationName, CustomerId, OrganizationId, ShareStatus, ModificationDateUtc, RecordDateUtc, State)
VALUES (:ClassificationId, :ClassificationName, :CustomerId, :OrganizationId, :ShareStatus, :ModificationDateUtc, :RecordDateUtc, :State);

insertOrReplaceSyncCustomerSegmentClassificationEntity:
