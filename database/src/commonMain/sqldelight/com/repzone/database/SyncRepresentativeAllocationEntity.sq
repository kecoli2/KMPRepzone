CREATE TABLE IF NOT EXISTS SyncRepresentativeAllocationEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    Amount REAL,
    Begin INTEGER,
    `End` INTEGER,
    ModificationDateUtc INTEGER,
    Name TEXT,
    OrganizationId INTEGER,
    RecordDateUtc INTEGER,
    RepresentativeId INTEGER,
    Scope INTEGER,
    State INTEGER
);

selectAllSyncRepresentativeAllocationEntity:
SELECT * FROM SyncRepresentativeAllocationEntity;

selectBySyncRepresentativeAllocationEntityId:
SELECT * FROM SyncRepresentativeAllocationEntity WHERE Id = ?;

insertSyncRepresentativeAllocationEntity:
INSERT INTO SyncRepresentativeAllocationEntity
VALUES ?;

insertOrReplaceSyncRepresentativeAllocationEntity:
INSERT OR REPLACE INTO SyncRepresentativeAllocationEntity
VALUES ?;

updateSyncRepresentativeAllocationEntity:
UPDATE SyncRepresentativeAllocationEntity SET
    OrganizationId = ?,
    Name = ?,
    Begin = ?,
    `End` = ?,
    Scope = ?,
    RepresentativeId = ?,
    Amount = ?,
    ModificationDateUtc = ?,
    RecordDateUtc = ?,
    State = ?
WHERE Id = ?;

deleteSyncRepresentativeAllocationEntity:
DELETE FROM SyncRepresentativeAllocationEntity WHERE Id = ?;
