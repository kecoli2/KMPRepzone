CREATE TABLE IF NOT EXISTS SyncRepresentativeAllocationEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    Amount REAL,
    Begin INTEGER,
    `End` INTEGER,
    ModificationDateUtc INTEGER,
    Name TEXT,
    OrganizationId INTEGER,
    RecordDateUtc INTEGER,
    RepresentativeId INTEGER,
    Scope INTEGER,
    State INTEGER
);

selectAllSyncRepresentativeAllocationEntity:
SELECT * FROM SyncRepresentativeAllocationEntity;

selectBySyncRepresentativeAllocationEntityId:
SELECT * FROM SyncRepresentativeAllocationEntity WHERE OrganizationId = :OrganizationId;

insertSyncRepresentativeAllocationEntity:
INSERT INTO SyncRepresentativeAllocationEntity (OrganizationId, Name, Begin, `End`, Scope, RepresentativeId, Amount, ModificationDateUtc, RecordDateUtc, State)
VALUES (:OrganizationId, :Name, :Begin, :End, :Scope, :RepresentativeId, :Amount, :ModificationDateUtc, :RecordDateUtc, :State);

insertOrReplaceSyncRepresentativeAllocationEntity:
INSERT OR REPLACE INTO SyncRepresentativeAllocationEntity (OrganizationId, Name, Begin, `End`, Scope, RepresentativeId, Amount, ModificationDateUtc, RecordDateUtc, State)
VALUES (:OrganizationId, :Name, :Begin, :End, :Scope, :RepresentativeId, :Amount, :ModificationDateUtc, :RecordDateUtc, :State);

updateSyncRepresentativeAllocationEntity:
UPDATE SyncRepresentativeAllocationEntity SET OrganizationId = :OrganizationId, Name = :Name, Begin = :Begin, `End` = :End, Scope = :Scope, RepresentativeId = :RepresentativeId, Amount = :Amount, ModificationDateUtc = :ModificationDateUtc, RecordDateUtc = :RecordDateUtc, State = :State WHERE OrganizationId = :OrganizationId;

deleteSyncRepresentativeAllocationEntity:
DELETE FROM SyncRepresentativeAllocationEntity WHERE OrganizationId = :OrganizationId;

deleteAllSyncRepresentativeAllocationEntity:
DELETE FROM SyncRepresentativeAllocationEntity;
