CREATE TABLE IF NOT EXISTS SyncAddressEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    AddressName TEXT,
    AddressType INTEGER,
    City TEXT,
    Contact TEXT,
    Country TEXT,
    CustomerId INTEGER,
    District TEXT,
    FaxNumber TEXT,
    Latitude REAL,
    Longitude REAL,
    PhoneNumber TEXT,
    State INTEGER,
    Street TEXT,
    Street2 TEXT
);