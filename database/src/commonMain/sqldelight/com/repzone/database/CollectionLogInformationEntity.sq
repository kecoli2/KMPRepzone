CREATE TABLE IF NOT EXISTS CollectionLogInformationEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    CollectionDate INTEGER,
    CustomerId INTEGER,
    DocumentUniqueId TEXT,
    Note TEXT,
    RestServiceTaskId INTEGER,
    SumTotal REAL
);

selectAllCollectionLogInformationEntity:
SELECT * FROM CollectionLogInformationEntity;

selectByCollectionLogInformationEntityId:
SELECT * FROM CollectionLogInformationEntity WHERE Id = :Id;

insertCollectionLogInformationEntity:
INSERT INTO CollectionLogInformationEntity (RestServiceTaskId, DocumentUniqueId, CustomerId, CollectionDate, SumTotal, Note)
VALUES (:RestServiceTaskId, :DocumentUniqueId, :CustomerId, :CollectionDate, :SumTotal, :Note);

insertOrReplaceCollectionLogInformationEntity:
INSERT OR REPLACE INTO CollectionLogInformationEntity (RestServiceTaskId, DocumentUniqueId, CustomerId, CollectionDate, SumTotal, Note)
VALUES (:RestServiceTaskId, :DocumentUniqueId, :CustomerId, :CollectionDate, :SumTotal, :Note);

updateCollectionLogInformationEntity:
UPDATE CollectionLogInformationEntity SET RestServiceTaskId = :RestServiceTaskId, DocumentUniqueId = :DocumentUniqueId, CustomerId = :CustomerId, CollectionDate = :CollectionDate, SumTotal = :SumTotal, Note = :Note WHERE Id = :Id;

