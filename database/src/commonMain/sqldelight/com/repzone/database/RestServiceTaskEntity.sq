CREATE TABLE IF NOT EXISTS RestServiceTaskEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    ActionName TEXT,
    CallType INTEGER,
    CreatedTime INTEGER,
    MethodPath TEXT,
    RequestObject TEXT,
    RequestObjectUniqueID TEXT,
    RequestType TEXT,
    Result TEXT,
    Status INTEGER
);

selectAllRestServiceTaskEntity:
SELECT * FROM RestServiceTaskEntity;

selectByRestServiceTaskEntityId:
SELECT * FROM RestServiceTaskEntity WHERE Id = :Id;

insertRestServiceTaskEntity:
INSERT INTO RestServiceTaskEntity (RequestObject, RequestType, MethodPath, CallType, Status, Result, ActionName, CreatedTime, RequestObjectUniqueID)
VALUES (:RequestObject, :RequestType, :MethodPath, :CallType, :Status, :Result, :ActionName, :CreatedTime, :RequestObjectUniqueID);

insertOrReplaceRestServiceTaskEntity:
INSERT OR REPLACE INTO RestServiceTaskEntity (RequestObject, RequestType, MethodPath, CallType, Status, Result, ActionName, CreatedTime, RequestObjectUniqueID)
