updateSyncCampaignMasterEntity:
UPDATE SyncCampaignMasterEntity SET Title = :Title, Start = :Start, `End` = :End, DocumentTypes = :DocumentTypes, GroupKey = :GroupKey, GroupPriority = :GroupPriority, IsOptional = :IsOptional, OrganizationId = :OrganizationId, MaxImplementationCount = :MaxImplementationCount, ModificationDateUtc = :ModificationDateUtc, RecordDateUtc = :RecordDateUtc, State = :State, ImplementationType = :ImplementationType, MaxImplementationAmount = :MaxImplementationAmount, ImplementationCalcType = :ImplementationCalcType WHERE OrganizationId = :OrganizationId;

