    ClassificationName TEXT,
    CustomerId INTEGER,
    ModificationDateUtc INTEGER,
    OrganizationId INTEGER,
    RecordDateUtc INTEGER,
    ShareStatus INTEGER,
    State INTEGER
);

selectAllSyncCustomerChannelClassificationEntity:
SELECT * FROM SyncCustomerChannelClassificationEntity;

selectBySyncCustomerChannelClassificationEntityId:
SELECT * FROM SyncCustomerChannelClassificationEntity WHERE ClassificationId = :ClassificationId;

insertSyncCustomerChannelClassificationEntity:
