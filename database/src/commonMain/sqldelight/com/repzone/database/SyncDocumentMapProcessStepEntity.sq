CREATE TABLE IF NOT EXISTS SyncDocumentMapProcessStepEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    Name TEXT,
    ObjectName TEXT,
    ProcessId INTEGER,
    State INTEGER,
    StepOrder INTEGER
);

selectAllSyncDocumentMapProcessStepEntity:
SELECT * FROM SyncDocumentMapProcessStepEntity;

selectBySyncDocumentMapProcessStepEntityId:
SELECT * FROM SyncDocumentMapProcessStepEntity WHERE Id = ?;

insertSyncDocumentMapProcessStepEntity:
INSERT INTO SyncDocumentMapProcessStepEntity
VALUES ?;

insertOrReplaceSyncDocumentMapProcessStepEntity:
INSERT OR REPLACE INTO SyncDocumentMapProcessStepEntity
VALUES ?;

updateSyncDocumentMapProcessStepEntity:
UPDATE SyncDocumentMapProcessStepEntity SET
    ProcessId = ?,
    Name = ?,
    ObjectName = ?,
    StepOrder = ?,
    State = ?
WHERE Id = ?;

deleteSyncDocumentMapProcessStepEntity:
