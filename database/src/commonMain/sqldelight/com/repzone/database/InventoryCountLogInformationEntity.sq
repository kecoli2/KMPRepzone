CREATE TABLE IF NOT EXISTS InventoryCountLogInformationEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    ApprovalDate INTEGER,
    Data TEXT,
    Direction INTEGER,
    TotalProductKind INTEGER,
    TotalProductQuantity REAL
);

selectAllInventoryCountLogInformationEntity:
SELECT * FROM InventoryCountLogInformationEntity;

selectByInventoryCountLogInformationEntityId:
SELECT * FROM InventoryCountLogInformationEntity WHERE Id = ?;

insertInventoryCountLogInformationEntity:
INSERT INTO InventoryCountLogInformationEntity
VALUES ?;

insertOrReplaceInventoryCountLogInformationEntity:
INSERT OR REPLACE INTO InventoryCountLogInformationEntity
VALUES ?;

updateInventoryCountLogInformationEntity:
UPDATE InventoryCountLogInformationEntity SET
    Data = ?,
    ApprovalDate = ?,
    TotalProductKind = ?,
    TotalProductQuantity = ?,
    Direction = ?
WHERE Id = ?;

deleteInventoryCountLogInformationEntity:
