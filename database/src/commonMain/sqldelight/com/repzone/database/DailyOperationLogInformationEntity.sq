CREATE TABLE IF NOT EXISTS DailyOperationLogInformationEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    BatteryLevel INTEGER,
    Date INTEGER,
    Description TEXT,
    LocalTime INTEGER,
    RepzoneLeaveRequestId INTEGER,
    RepzoneLeaveRequestUniqueId TEXT,
    Type INTEGER
);

selectAllDailyOperationLogInformationEntity:
SELECT * FROM DailyOperationLogInformationEntity;

selectByDailyOperationLogInformationEntityId:
SELECT * FROM DailyOperationLogInformationEntity WHERE Id = ?;

insertDailyOperationLogInformationEntity:
INSERT INTO DailyOperationLogInformationEntity
VALUES ?;

insertOrReplaceDailyOperationLogInformationEntity:
INSERT OR REPLACE INTO DailyOperationLogInformationEntity
VALUES ?;

updateDailyOperationLogInformationEntity:
