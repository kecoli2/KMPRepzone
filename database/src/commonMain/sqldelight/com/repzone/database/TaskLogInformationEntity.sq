CREATE TABLE IF NOT EXISTS TaskLogInformationEntity (
    Id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    CustomerId INTEGER,
    Date INTEGER,
    FileUrl TEXT,
    FormDataUniqueId TEXT,
    FormId INTEGER,
    Latitude REAL,
    Longitude REAL,
    Note TEXT,
    Status INTEGER,
    StepId INTEGER,
    TaskId INTEGER,
    UniqueId TEXT,
    VisitId INTEGER,
    VisitUniqueId TEXT
);

selectAllTaskLogInformationEntity:
SELECT * FROM TaskLogInformationEntity;

selectByTaskLogInformationEntityId:
SELECT * FROM TaskLogInformationEntity WHERE Id = :Id;

insertTaskLogInformationEntity:
INSERT INTO TaskLogInformationEntity (TaskId, StepId, Latitude, Longitude, FormId, FileUrl, Date, Status, Note, CustomerId, VisitId, VisitUniqueId, UniqueId, FormDataUniqueId)
VALUES (:TaskId, :StepId, :Latitude, :Longitude, :FormId, :FileUrl, :Date, :Status, :Note, :CustomerId, :VisitId, :VisitUniqueId, :UniqueId, :FormDataUniqueId);

insertOrReplaceTaskLogInformationEntity:
