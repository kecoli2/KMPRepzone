updateTaskLogInformationEntity:
UPDATE TaskLogInformationEntity SET TaskId = :TaskId, StepId = :StepId, Latitude = :Latitude, Longitude = :Longitude, FormId = :FormId, FileUrl = :FileUrl, Date = :Date, Status = :Status, Note = :Note, CustomerId = :CustomerId, VisitId = :VisitId, VisitUniqueId = :VisitUniqueId, UniqueId = :UniqueId, FormDataUniqueId = :FormDataUniqueId WHERE Id = :Id;

