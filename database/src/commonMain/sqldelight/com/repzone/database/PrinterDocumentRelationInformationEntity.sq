    PaperWidth INTEGER,
    PrinterAddress TEXT,
    PrinterName TEXT,
    PrinterType INTEGER,
    State INTEGER
);

selectAllPrinterDocumentRelationInformationEntity:
SELECT * FROM PrinterDocumentRelationInformationEntity;

selectByPrinterDocumentRelationInformationEntityId:
SELECT * FROM PrinterDocumentRelationInformationEntity WHERE DocumentMapId = :DocumentMapId;

insertPrinterDocumentRelationInformationEntity:
INSERT INTO PrinterDocumentRelationInformationEntity (PrinterName, PrinterAddress, State, PrinterType, PaperWidth)
VALUES (:PrinterName, :PrinterAddress, :State, :PrinterType, :PaperWidth);

insertOrReplacePrinterDocumentRelationInformationEntity:
INSERT OR REPLACE INTO PrinterDocumentRelationInformationEntity (PrinterName, PrinterAddress, State, PrinterType, PaperWidth)
