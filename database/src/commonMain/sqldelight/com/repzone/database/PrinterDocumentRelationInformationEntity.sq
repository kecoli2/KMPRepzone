CREATE TABLE IF NOT EXISTS PrinterDocumentRelationInformationEntity (
    DocumentMapId INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    PaperWidth INTEGER,
    PrinterAddress TEXT,
    PrinterName TEXT,
    PrinterType INTEGER,
    State INTEGER
);

selectAllPrinterDocumentRelationInformationEntity:
SELECT * FROM PrinterDocumentRelationInformationEntity;

selectByPrinterDocumentRelationInformationEntityId:
SELECT * FROM PrinterDocumentRelationInformationEntity WHERE DocumentMapId = ?;

insertPrinterDocumentRelationInformationEntity:
INSERT INTO PrinterDocumentRelationInformationEntity
VALUES ?;

insertOrReplacePrinterDocumentRelationInformationEntity:
INSERT OR REPLACE INTO PrinterDocumentRelationInformationEntity
VALUES ?;

updatePrinterDocumentRelationInformationEntity:
UPDATE PrinterDocumentRelationInformationEntity SET
    PrinterName = ?,
    PrinterAddress = ?,
    State = ?,
    PrinterType = ?,
    PaperWidth = ?
WHERE DocumentMapId = ?;

deletePrinterDocumentRelationInformationEntity:
DELETE FROM PrinterDocumentRelationInformationEntity WHERE DocumentMapId = ?;
